# and open the template in the editor.
%>

<%#= "_header.html" %>
<header>
<div id="header" class="notice_header">
  <div class="center">

    <div class="inner">
      <h1><a href="/">Pivotal Tracker</a></h1>
      <ul class="nav_links">




            <% if !signedin? %>

            <li class="dashboard">
              <%#*<a class="current">Dashboard</a>%>
              <%= link_to "Dashboard", stories_path,:class => "current" %>
            </li>
            <li>
              <%#*<a href="https://www.pivotaltracker.com/reports/progress">Reports</a>%>
              <%= link_to "Reports",'#' %>
            </li>
            <li>
              <%#*<a href="https://www.pivotaltracker.com/profile">Profile</a>%>
              <%link_to "Profile", '#'%>
            </li>
            <li>
              <%#*<li><a href="https://www.pivotaltracker.com/accounts/338789/plans">Account</a>%>
              <%= link_to "Account",edit_user_path(current_user.id) %>
            </li>
            <li>
              <%#*<a href="https://www.pivotaltracker.com/help">Help</a>%>
              <%= link_to "Help", '#' %>
            </li>
            <li>
              <%= link_to "Signout",signout_path, :method => :delete  %>
            </li>
            <li style="color: white">
              Welcome <br/>
              <%= current_user.name %>
            </li>
            <%else%>
              <li>
              <%#*<a href="https://www.pivotaltracker.com/features">Features</a>%>
              <%= link_to "Features","#" %>
            </li>
            <li>
              <%#*<a href="https://www.pivotaltracker.com/testimonials">Clients &amp; Testimonials</a>%>
              <%= link_to "Clients Testimonials" , '#' %>
            </li>
            <li>
              <%#*<a href="https://www.pivotaltracker.com/public_projects">Public Projects</a>%>
              <%= link_to "Public Projects",'#' %>
            </li>

            <li>
              <%#*<a href="https://www.pivotaltracker.com/pricing">Pricing</a>%>
              <%= link_to "Pricing",'#' %>
            </li>
            <li><%= link_to "SignIn",signin_path%></li>
            
            <%end%>
          <!--<a href="https://www.pivotaltracker.com/signin" tabindex="1">Sign In</a>-->
      </ul>
    </div>
  </div>



</div>
</header>
