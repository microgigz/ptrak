
<head>

  <title>My Projects - Pivotal Tracker</title>

  <link href="projects_files/non_stories_view_prod-release-20111025-20111221-3_pkg.css" media="screen" rel="Stylesheet" type="text/css"/>
  <script src="projects_files/ga.js" async="" type="text/javascript"></script><script src="projects_files/non_stories_view_prod-release-20111025-20111221-3_pkg.js" type="text/javascript"></script>

  <script type="text/javascript">
    //<![CDATA[
    window._gaq = window._gaq || [];
    window._gaq.push(['_setAccount', 'UA-1687866-3']);
    window._gaq.push(['_trackPageview']);



    (function() {
      var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
      ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
      var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();

    //]]>
  </script>


</head>
<body class="yui-skin-sam projects_page">
  <div style="display: none;"><img alt="" src="projects_files/actions_overlay_bg.png"><img alt="" src="projects_files/create_project_button.png"><img alt="" src="projects_files/create_project_button_hover.png"></div>
  <div id="main">
    <script type="text/javascript">
      ENVIRONMENT='prod'
    </script>




    <div class="content clearfix">
      <div id="flash">

        <ul>



        </ul>

      </div>



      <div class="projects_page">

        <h1>
          My Projects
        </h1>


        <div class="projects_list">
          <div class="content_section has_bottom_spacer">

            <div class="tabular_data">
              <div class="header">
                <h3><%= current_user.name %></h3>

                <a class="header_button right_link" href="#" id="new_project_link" onclick="    SimpleOverlay.create(function(overlay){
                  overlay.overlay.setOpacity(0.3);
                  overlay.onCancel = function() { $$('.add_project_error_message').each(function(el){el.innerHTML = '';}) };

                  img = $$('#new_project_popup .close_image').first();
                  YAHOO.util.Event.addListener(img, 'click', overlay.cancel.bind(overlay));

                  return $('new_project_popup');
                });
                
                $('new_project_popup').show();
                setTimeout(function() { $$('#new_project_popup .project_name_field').first().focus(); }, 50);
                jQuery(&quot;label.infield&quot;).inFieldLabels({fadeDuration: 100});
                ; return false;">Create Project</a>
                <div class="new_project_popup" id="new_project_popup" style="display:none">
                  <%=render "form"%>
                  
                  <div class="popup_foot"></div>

                </div>


                <a href="#" class="header_button right_link" id="manage_account_link_338789">Manage Account</a>

              </div>
              <ul class="rows">

                <% @projects.each do |project| %>
                  <li>
                    <div class="title">

                      <a href="https://#/projects/432771" class="project_name"><%= project.pro_name %></a>

                    </div>

                    <div class="actions"><a href="#">Overview</a></div>
                    <div class="actions"><a href="#">View Stories</a></div>
                    <div class="actions"><a href="#">Settings</a></div>
                    <div class="actions"><a href="#">Members</a></div>


                    <div class="actions"> <a href="https://#/projects/432771/archive" onclick="if (confirm('You are about to archive the project \&quot;bibleography.\&quot; Are you sure you want to do this?')) { var f = document.createElement('form'); f.style.display = 'none'; this.parentNode.appendChild(f); f.method = 'POST'; f.action = this.href;f.submit(); };return false;">Archive</a> </div>



                    <div class="actions"> <a href="https://#/projects/432771" onclick="if (confirm('You are about to permanently delete the project \&quot;bibleography.\&quot; Are you sure you want to do this?')) { var f = document.createElement('form'); f.style.display = 'none'; this.parentNode.appendChild(f); f.method = 'POST'; f.action = this.href;var m = document.createElement('input'); m.setAttribute('type', 'hidden'); m.setAttribute('name', '_method'); m.setAttribute('value', 'delete'); f.appendChild(m);f.submit(); };return false;">Delete</a> </div>



                    <div class="project_stats">*&nbsp;Updated after adding members</div>%><%# un comments after aadding members%>
                    <div class="project_stats">*&nbsp;Updated after adding Stories</div>%><%# un comments after aadding Storoies%>
                    <%= image_tag "/projects/gear_myprojects.png", :alt => 'settings',:class => 'settings' %>
                  </li>
                <%end%>
              </ul>
            </div>

          </div>
        </div>

      </div>
    </div>
    <noscript>
      <div id='js_required'>Tracker requires JavaScript. Please enable it in your browser.</div>
    </noscript>
    <div class="push"></div>
  </div>
  <div id="footer">
    <div id="footer_content">
      <a href="https://pivotallabs.com/" target="_blank">About Pivotal Labs</a> | <a href="https://#/help">Help</a> | <a href="https://#/help">Feedback</a> | <a href="http://pivotallabs.com/tracker" target="_blank">Blog</a> | <a href="https://#/policy/eula">Policies</a> | <a href="mailto:tracker@pivotallabs.com">Contact Us</a>
      <a href="https://www.twitter.com/pivotaltracker" id="twitter_link" target="_blank">Follow Pivotal Tracker on Twitter</a>
    </div>
  </div>





  <script type="text/javascript">
  //<![CDATA[
  onload = function(){};
  //]]>
  </script>

</body>