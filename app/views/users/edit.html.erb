<%= form_for @user do |f| %>


  <%= f.label :name  %>
  <%= f.text_field :name%><br />



  <%= f.label :email  %>
  <%= f.text_field :email%><br />



  <%= f.label :e_password  %>
  <%= f.password_field :e_password%><br />


  <%= f.submit "Update" %>


<%end%>
